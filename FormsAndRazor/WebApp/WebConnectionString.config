<connectionStrings>
  <!--name this is the string that you will reference on your web form to point to the database you are interested in connectionString 3 parameters
          Data Source indicates the sql Server Name
          Initial Catalog indicate the Sql database name
          Integrated Security set to true
      ProviderName indicates the type of client your database needs (we have an sql database therefore our client is SqlClient-->
  <add name="NWDB"
        connectionString="Data Source=.; Initial Catalog=Northwind_CPSC1517; Integrated Security=true"
        providerName="System.Data.SqlClient"/>
</connectionStrings>